  "version": "1.2.0",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "start": "ts-node -r tsconfig-paths/require src/index.ts",
    "dev": "nodemon -r tsconfig-paths/require src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "jest"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",

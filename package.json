  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "dotenv-expand": "^9.0.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
